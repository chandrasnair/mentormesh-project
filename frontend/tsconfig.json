{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "target": "esnext",
    "module": "esnext"
  },
  "include": ["src/**/*"]
}
